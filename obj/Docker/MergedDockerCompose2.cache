services:
  rabbitmq:
    container_name: rabbitmq
    environment:
      RABBITMQ_DEFAULT_PASS: guest
      RABBITMQ_DEFAULT_USER: guest
    hostname: rabbitmq
    image: rabbitmq:3.7-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
  sample.server:
    build:
      context: C:\GitHub\SimpleCQRS
      dockerfile: Sample.Client/Dockerfile
    image: sampleserver
version: '3.4'
services:
  rabbitmq:
    container_name: rabbitmq
    environment:
      RABBITMQ_DEFAULT_PASS: guest
      RABBITMQ_DEFAULT_USER: guest
    hostname: rabbitmq
    image: rabbitmq:3.7-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
  sample.client:
    build:
      context: C:\GitHub\SimpleCQRS
      dockerfile: Sample.Client/Dockerfile
    image: sampleclient
  sample.server:
    build:
      context: C:\GitHub\SimpleCQRS
      dockerfile: Sample.Server/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Sample.Server
      target: base
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: sampleserver:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.2/Sample.Server.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    volumes:
    - C:\GitHub\SimpleCQRS\Sample.Server:/app:rw
    - C:\Users\ThiagoLoureiro\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\ThiagoLoureiro\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'